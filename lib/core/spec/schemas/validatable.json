{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "title",
    "button",
    "closed_pinata_img_url",
    "no_of_taps"
  ],
  "properties": {
    "title": {
      "type": "string",
      "example": "Play and get rewarded!"
    },
    "sub_title": {
      "type": "string",
      "example": "Try until you win"
    },
    "button": {
      "type": "string",
      "example": "Start playing",
      "maxLength": 20
    },
    "closed_pinata_img_url": {
      "type": "string",
      "format": "url"
    },
    "number_of_taps": {
      "type": "number",
      "min": 1,
      "max": 10
    }
  }
}
